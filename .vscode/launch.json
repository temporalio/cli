{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "serve in memory",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/temporal",
      "cwd": "${workspaceFolder}",
      "args": [
        "server",
        "start-dev",
      ],
      "env": {
        "CGO_ENABLED": "0",
      }
    },
    {
      "name": "workflow list",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/temporal",
      "cwd": "${workspaceFolder}",
      "args": [
        "workflow",
        "list",
      ]
    }
  ],
}
